<h1>Events Page for <%= @user.name %>:</h1>
<div>
  <h3> Hosted </h2>
  <ul>
    <% @user.hosted_events.each do |event| %>
        <li><%= event.title %> | <%= event.location %></li>
    <% end %>
  </ul>
  <h3>Attending </h2>
  <ul>
    <% @user.attended_events.each do |event| %>
        <li><%= event.title %> | <%= event.location %></li>
    <% end %>
  </ul>
  
</div>

